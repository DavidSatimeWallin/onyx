#load "core/std"

use package core

main :: (args: [] cstr) {
    ba := bucket_array.make(i32, 4);
    for i: 24 do ba << i;

    printf("ba[10] is {}.\n", ba[10]);

    sum := 0;
    bucket_array.for_each(ba, #quote {
        printf("[{}] -> {}\n", bucket_index, *it);
        sum += *it;
    });

    for it: bucket_array.as_iter(&ba) {
        printf("{}\n", it);
    }

    printf("Sum is {}\n", sum);
}
