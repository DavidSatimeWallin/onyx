package main

#load "core/std"

use package core

main :: proc (args: [] cstr) {
    imap : map.Map(i32, str);
    map.init(^imap, "");
    defer {
        print("Freeing map\n");
        map.free(^imap);
    }
    map.put(^imap, 50, "Hello ");
    map.put(^imap, 1234, "World!");

    println(map.has(^imap, 50));
    println(map.has(^imap, 51));

    printf("%s%s\n", map.get(^imap, 50), map.get(^imap, 1234));

    map.delete(^imap, 50);
    println(map.has(^imap, 50));

    map.clear(^imap);
    println(map.has(^imap, 1234));
}
