package main

#load "core/std"

use core {*}

main :: (args: [] cstr) {
    imap : Map(i32, str);
    map.init(&imap);
    defer {
        print("Freeing map\n");
        map.free(&imap);
    }
    map.put(&imap, 50, "Hello ");
    imap->put(1234, "World!");

    println(map.has(&imap, 50));
    println(map.has(&imap, 51));

    printf("{}{}\n", map.get(&imap, 50), map.get(&imap, 1234));

    printf("{*p}\n", &imap);

    map.delete(&imap, 50);
    println(map.has(&imap, 50));

    map.clear(&imap);
    println(map.has(&imap, 1234));
}
