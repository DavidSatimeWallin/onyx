package json
use package core

decode_string :: (data: str, allocator := context.allocator) -> Json {
    tokenizer := Tokenizer.{ data = data };

    err: Tokenizer_Error = .None;
    tkn: Token;

    while err == .None {
        tkn, err = token_get(^tokenizer);

        buf: [10] u8;
        printf("%s  %s\n", conv.i64_to_str(~~tkn.kind, 10, ~~buf, 2), tkn.text);
    }
}

