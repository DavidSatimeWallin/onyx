use "progs/intrinsics"

pointer_test :: proc {
    p := 0 as ^i32;
}

print :: foreign "host" "print" proc (val: i32) ---

test :: proc (a: bool) -> bool {
    return !a;
}

foo :: proc (n: i32) -> i32 {
    a :: shl_i32(n, 1);
    b :: shl_i32(n, 2);
    return or_i32(a, b);
}

export main :: proc {
    a := 16;
    print(clz_i32(a));

    b := 1 + foo(2);
    print(b);

    if b == 13
        ---
    else {
        print(1024);
    }

    cond :: true;
    print(test(cond) as i32);
}
