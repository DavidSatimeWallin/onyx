
print :: foreign "host" "print" proc (val i32) ---

export main :: proc {

    while true {
        print(8000);
        break;
    }

    x := 0;
    while x < 10 {

        if x == 4 {

            x := 1;
            while x < 1000000 {
                print(x);

                if x > 4 { break; }

                x = x + 1;
            }

        }

        if x == 2 {

            x := 1;
            while x <= 5 {
                if x == 3 {
                    x = x + 1;
                    continue;
                }

                print(x);
                x = x + 1;
            }

        }

        x = x + 1;
    }

}
