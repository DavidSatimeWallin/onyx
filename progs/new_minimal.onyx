print :: foreign "host" "print" proc (value i32) ---

simple_test :: proc {
    a: i32 = 5;
    b: i64 = 6 as i64;

    if a > b {
        foo := 123;
        print(foo);
    }

    c :: a + foo();
    print(c);
}

foo :: proc -> i32 {
    return 10;
}

print_nums :: proc (a i32, b i32, c i32) {
    print(a);
    print(b);
    print(c);
}
