print :: foreign "host" "print" proc (value: i32) ---

simple_test :: proc {
    a: i32 = 5;
    b: i32 = 6;

    if a > b {
        foo := 123;
        print(foo);
    }

    c :: a + foo();
    print(c);
}

fib :: proc (n: i32) -> i32 {
    if n <= 1 { return 1; }

    return fib(n - 1) + fib(n - 2);
}

foo :: proc -> i32 {
    return 10;
}

print_nums :: proc (a: i32, b: i32, c: i32) {
    print(a);
    print(b);
    print(c);
}
