foo :: proc () -> i32 {
	return 10 as i32;
}

add :: proc (a i32, b i32) -> i32 {
	return a + b;
}

diff_square :: proc (a i32, b i32) -> i32 {
	// Typechecked
	c := a - b; // Mutable
	d :: a + b; // Constant

	return (c * d) as i32;
}

export do_stuff :: proc () -> i32 {
	res := diff_square((2 + 3) as i32, (4 + 5) as i32);
	res = res + foo();
    return res;
}

export main :: proc () {
    output :: do_stuff();
}
