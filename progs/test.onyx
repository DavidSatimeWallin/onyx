
// Foreign functions are included this way:
//      sym_name :: foreign "module" "name" proc ...

print       :: foreign "host" "print" proc (value i32) ---

something_else :: proc -> i32 {
    return 100;
}

// This is the entry point
export main :: proc {
    nineteen :: 5 * 3 + 4;
    thirty_five :: 5 * (3 + 4);

    something :: other_value();

    print(nineteen);
    print(thirty_five);
    print(something);
}
