use "progs/print_funcs"
use "progs/other"

something_else :: proc (n: i32) -> i32 {
    return 100 * n + global_value;
}

global_value := 100

export in_unit_circle :: proc (x: f32, y: f32) -> bool {
    return (x * x) + (y * y) < 1.0f;
}

sqrt_f32 ::
    proc #intrinsic
    (val: f32) -> f32 ---

// This is the entry point
export main2 :: proc {
    i := 0;
    while i < 10 {
        res :: fib(i);
        print_i32(res);
        i += 1;
    }

    i = 0;
    while i < 10 {
        res :: factorial(i);
        print_i32(res);
        i += 1;
    }

    x : i32;
    y := 0;
    while y < 5 {

        x = 0;
        while x < 5 {
            if x == 3 {
                x += 1;
                continue;
            }

            print_i32((x + y * 5) % 10);
            x += 1;
        }

        if y > 2 { break; }

        y += 1;
    }
}

