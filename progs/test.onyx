use "progs/print_funcs"
use "progs/other"

something_else :: proc (n: i32) -> i32 {
    return 100 * n + global_value;
}

global_value := 100

export in_unit_circle :: proc (x: f32, y: f32) -> bool {
    return (x * x) + (y * y) < 1.0f;
}

// This is the entry point
export main2 :: proc {
    i := 0;
    while i < 10 {
        res :: fib(i);
        print_i32(res);
        i = i + 1;
    }

    i = 0;
    while i < 10 {
        res :: factorial(i);
        print_i32(res);
        i = i + 1;
    }

    x : i32;
    y := 0;
    while y < 5 {

        x = 0;
        while x < 5 {
            if x == 3 {
                x = x + 1;
                continue;
            }

            print_i32((x + y * 5) % 10);
            x = x + 1;
        }

        if y > 2 { break; }

        y = y + 1;
    }
}

export main :: proc {
    print_bool(in_unit_circle(0.5f, 0.5f));

    big_num := fib(factorial(4));
    something :: other_value(0);

    global_value = 1000;
    something_else :: other_value(0);

    condition := big_num < something;

    if condition {
        print_i32(big_num);
        print_i32(something);
        print_i32(something_else);
    }
}

