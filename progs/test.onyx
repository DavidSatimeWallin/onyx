export main :: proc {
    print_f32(sqrt_f32(2.0f));
    print_i32(5 * 6 + 2 * 3);
    print_bool(in_unit_circle(0.5f, 0.5f));

    big_num := fib(factorial(4));
    something :: other_value(0);

    global_value = 1000;
    something_else :: other_value(0);

    condition := big_num < something;

    if condition {
        print_i32(big_num);
        print_i32(something);
        print_i32(something_else);
    }
}

